    "        # Calcula o tempo de espera e inicia a execu√ß√£o\n",
    "        wait_time = time.perf_counter() - wait_start\n",
    "        print(f'Modalidade {title:<30} | Tempo de espera: {wait_time:.3f} horas')\n",
